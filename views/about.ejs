<%- include('header') %>
    <main id="content" class="site-main">
        <!-- Inner Banner html start-->
        <section class="inner-banner-wrap">
            <div class="inner-baner-container" style="background-image: url(/images/principal1.jpg);">
                <div class="overlay balance-patten"></div>
                <div class="container">
                    <div class="inner-banner-content">
                        <!-- <h1 class="inner-title"><%= __("about.title") %></h1> -->
                    </div>
                </div>
            </div>
        </section>
        <!-- Inner Banner html end-->
        <!-- About page html start -->
        <section class="about-section">
            <div class="container">
                <h2 class="about-title text-center"><%= __("about.presentation_title") %></h2>
                <div class="row">
                    <div class="col-md-6">
                        <p class="about-describe info-left">
                            <%- __("about.paragraph1") %>
                            <br><br>
                            <%- __("about.paragraph2") %>
                        </p>
                    </div>
                    <div class="col-md-6">
                        <p class="about-describe info-right">
                            <%- __("about.paragraph3") %>
                            <br><br>
                            <%- __("about.paragraph4") %>
                        </p>
                    </div>
                </div>
                <div class="counter-inner">
                    <div class="counter-item-wrap row">
                        <div class="col-lg-3 col-sm-6 counter-item">
                            <div class="counter-no">
                                <span class="counter">2</span>k+
                            </div>
                            <div class="Completed">
                                <%= __("about.stats.consultations") %>
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-6 counter-item">
                            <div class="counter-no">
                                <span class="counter">95</span>%
                            </div>
                            <div class="Completed">
                                <%= __("about.stats.success_rate") %>
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-6 counter-item">
                            <div class="counter-no">
                                <span class="counter">12</span>+
                            </div>
                            <div class="Completed">
                                <%= __("about.stats.lawyers") %>
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-6 counter-item">
                            <div class="counter-no">
                                <span class="counter">20</span>+
                            </div>
                            <span class="Completed">
                                <%= __("about.stats.awards") %>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="about-banner">
                    <figure class="about-image">
                        <img src="/images/legalor-img18.jpg" alt="J&J Law Firm">
                    </figure>
                    <div class="about-left-banner">
                        <div class="banner-icon">
                            <i aria-hidden="true" class="icon icon-trophy1"></i>
                        </div>
                        <h3><%= __("about.experience_badge") %></h3>
                    </div>
                    <div class="about-right-banner"></div>
                </div>
            </div>
        </section>
        <div class="slider-section about-slider">
            <div class="container">
                <div class="client-slider text-center">
                    <div class="client-item">
                        <figure>
                            <img src="/images/legalor-img20.png" alt="Client logo">
                        </figure>
                    </div>
                    <div class="client-item">
                        <figure>
                            <img src="/images/legalor-img21.png" alt="Client logo">
                        </figure>
                    </div>
                    <div class="client-item">
                        <figure>
                            <img src="/images/legalor-img22.png" alt="Client logo">
                        </figure>
                    </div>
                    <div class="client-item">
                        <figure>
                            <img src="/images/legalor-img23.png" alt="Client logo">
                        </figure>
                    </div>
                    <div class="client-item">
                        <figure>
                            <img src="/images/legalor-img24.png" alt="Client logo">
                        </figure>
                    </div>
                </div>
            </div>
        </div>
        <!-- About page html end -->
        <!-- about schedule section html start -->
        <section class="home-schedule about-schedule">
            <div class="overlay gradient-overlay"></div>
            <div class="container">
                <div class="schedule-wrapper">
                    <div class="right-side-schedule">
                        <aside class="section-head">
                            <h2 class="section-title">
                                <%= __("home.schedule.title") %>
                            </h2>
                            <p class="section-paragraph">
                                <%= __("home.schedule.subtitle") %>
                            </p>
                            <div class="timenote">
                                <div class="schedule-time">
                                    <ul>
                                        <li>
                                            <h6><%= __("home.schedule.hours.weekdays") %></h6>
                                        </li>
                                        <li>
                                            <h6><%= __("home.schedule.hours.hours") %></h6>
                                        </li>
                                        <li>
                                            <h6><%= __("home.schedule.hours.closed") %></h6>
                                        </li>
                                    </ul>
                                </div>
                                <div class="note-list">
                                    <ul>
                                        <li>
                                            <i aria-hidden="true" class="far fa-check-square"></i>
                                            <span>
                                                <%= __("home.schedule.commitments.client") %>
                                            </span>
                                        </li>
                                        <li>
                                            <i aria-hidden="true" class="far fa-check-square"></i>
                                            <span>
                                                <%= __("home.schedule.commitments.strategic") %>
                                            </span>
                                        </li>
                                        <li>
                                            <i aria-hidden="true" class="far fa-check-square"></i>
                                            <span>
                                                <%= __("home.schedule.commitments.integrity") %>
                                            </span>
                                        </li>
                                        <li>
                                            <i aria-hidden="true" class="far fa-check-square"></i>
                                            <span>
                                                <%= __("home.schedule.commitments.navigate") %>
                                            </span>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="schedule-btn">
                                <a href="/contact" class="button-primary"><%= __("common.hire_us_now") %></a>
                            </div>
                        </aside>
                    </div>
                    <div class="left-side-shedule">
                        <div class="goal-wrapper">
                            <div class="circle-progressbar-yellow circle-progressbar" data-animate="false">
                                <div class="circle-bar" data-percent="95">
                                    <div class="circle-number"></div>
                                </div>
                            </div>
                            <h5 class="goal-title"><%= __("home.schedule.legal_solution") %></h5>
                        </div>
                        <div class="goal-wrapper">
                            <div class="circle-progressbar-white circle-progressbar" data-animate="false">
                                <div class="circle-bar" data-percent="98">
                                    <div class="circle-number"></div>
                                </div>
                            </div>
                            <h5 class="goal-title"><%= __("home.schedule.project_success") %></h5>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
    <!-- footer part -->
<%- include('footer') %>
