<%- include('header') %>
    <main id="content" class="site-main">
        <!-- Inner Banner html start-->
        <section class="inner-banner-wrap">
            <div class="inner-baner-container" style="background-image: url(/images/principal1.jpg);">
                <div class="overlay balance-patten"></div>
                <div class="container">
                    <div class="inner-banner-content">
                        <!-- <h1 class="inner-title"><%= __('feature_page.title') %></h1> -->
                    </div>
                </div>
            </div>
        </section>
        <!-- Inner Banner html end-->
        <!-- feature page html start -->
        <section class="feature-section">
            <div class="container">
                <div class="featuer-service-content">
                    <!-- Card 1 -->
                    <div class="serice-type">
                        <div class="service-card">
                            <h5 class="service-header">
                                <%= __('feature_page.card_1_title') %>
                            </h5>
                            <figure class="servie-icon">
                                <i class="fas fa-award"></i>
                            </figure>
                            <p class="service-desc">
                                <%= __('feature_page.card_1_desc') %>
                            </p>
                            <a href="/attorney" class="service-link">
                                <i class="fas fa-arrow-right"></i> <%= __('feature_page.card_1_link') %>
                            </a>
                        </div>
                    </div>
                    
                    <!-- Card 2 -->
                    <div class="serice-type active">
                        <div class="service-card">
                            <h5 class="service-header">
                                <%= __('feature_page.card_2_title') %>
                            </h5>
                            <figure class="servie-icon">
                                <i class="fas fa-gavel"></i>
                            </figure>
                            <p class="service-desc">
                                <%= __('feature_page.card_2_desc') %>
                            </p>
                            <a href="/practice-area" class="service-link">
                                <i class="fas fa-arrow-right"></i> <%= __('feature_page.card_2_link') %>
                            </a>
                        </div>
                    </div>
                    
                    <!-- Card 3 -->
                    <div class="serice-type">
                        <div class="service-card">
                            <h5 class="service-header">
                                <%= __('feature_page.card_3_title') %>
                            </h5>
                            <figure class="servie-icon">
                                <i class="fas fa-hand-holding-usd"></i>
                            </figure>
                            <p class="service-desc">
                                <%= __('feature_page.card_3_desc') %>
                            </p>
                            <a href="/contact" class="service-link">
                                <i class="fas fa-arrow-right"></i> <%= __('feature_page.card_3_link') %>
                            </a>
                        </div>
                    </div>
                    
                    <!-- Card 4 -->
                    <div class="serice-type">
                        <div class="service-card">
                            <h5 class="service-header">
                                <%= __('feature_page.card_4_title') %>
                            </h5>
                            <figure class="servie-icon">
                                <i class="fas fa-handshake"></i>
                            </figure>
                            <p class="service-desc">
                                <%= __('feature_page.card_4_desc') %>
                            </p>
                            <a href="/testimonial" class="service-link">
                                <i class="fas fa-arrow-right"></i> <%= __('feature_page.card_4_link') %>
                            </a>
                        </div>
                    </div>
                    
                    <!-- Card 5 -->
                    <div class="serice-type">
                        <div class="service-card">
                            <h5 class="service-header">
                                <%= __('feature_page.card_5_title') %>
                            </h5>
                            <figure class="servie-icon">
                                <i class="fas fa-balance-scale"></i>
                            </figure>
                            <p class="service-desc">
                                <%= __('feature_page.card_5_desc') %>
                            </p>
                            <a href="/reservation" class="service-link">
                                <i class="fas fa-arrow-right"></i> <%= __('feature_page.card_5_link') %>
                            </a>
                        </div>
                    </div>
                    
                    <!-- Card 6 -->
                    <div class="serice-type">
                        <div class="service-card">
                            <h5 class="service-header">
                                <%= __('feature_page.card_6_title') %>
                            </h5>
                            <figure class="servie-icon">
                                <i class="fas fa-user-graduate"></i>
                            </figure>
                            <p class="service-desc">
                                <%= __('feature_page.card_6_desc') %>
                            </p>
                            <a href="/about" class="service-link">
                                <i class="fas fa-arrow-right"></i> <%= __('feature_page.card_6_link') %>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- feature working operation html -->
        <section class="featuer-working-operation">
            <div class="container">
                <div class="working-operation-wrapper">
                    <div class="operation-info">
                        <div class="section-head">
                            <h2 class="section-title">
                                <%= __('feature_page.method_title') %>
                            </h2>
                            <p class="section-paragraph">
                                <%= __('feature_page.method_paragraph') %>
                            </p>
                            <div class="stats-grid">
                                <div class="stat-item">
                                    <div class="stat-number">9</div>
                                    <div class="stat-label"><%= __('feature_page.stat_specializations') %></div>
                                </div>
                                <div class="stat-item">
                                    <div class="stat-number">15+</div>
                                    <div class="stat-label"><%= __('feature_page.stat_experience') %></div>
                                </div>
                                <div class="stat-item">
                                    <div class="stat-number">95%</div>
                                    <div class="stat-label"><%= __('feature_page.stat_success_rate') %></div>
                                </div>
                                <div class="stat-item">
                                    <div class="stat-number">2000+</div>
                                    <div class="stat-label"><%= __('feature_page.stat_clients') %></div>
                                </div>
                            </div>
                            <div class="section-btn">
                                <a href="/practice-area" class="button-primary"><%= __('feature_page.btn_specialties') %></a>
                                <a href="/contact" class="button-secondary"><%= __('feature_page.btn_contact') %></a>
                            </div>
                        </div>
                    </div>
                    <div class="operation-chart">
                        <div class="goal-wrapper">
                            <div class="circle-progressbar-dark circle-progressbar" data-animate="false">
                                <div class="circle-bar" data-percent="97">
                                    <div class="circle-number"></div>
                                </div>
                            </div>
                            <h5 class="goal-title"><%= __('feature_page.goal_1_title') %></h5>
                            <span class="goal-info">
                                <%= __('feature_page.goal_1_desc') %>
                            </span>
                            <a href="/reservation" class="goal-link"><%= __('feature_page.goal_1_link') %></a>
                        </div>
                        <div class="goal-wrapper">
                            <div class="circle-progressbar-dark circle-progressbar" data-animate="false">
                                <div class="circle-bar" data-percent="95">
                                    <div class="circle-number"></div>
                                </div>
                            </div>
                            <h5 class="goal-title"><%= __('feature_page.goal_2_title') %></h5>
                            <span class="goal-info">
                                <%= __('feature_page.goal_2_desc') %>
                            </span>
                            <a href="/practice-area" class="goal-link"><%= __('feature_page.goal_2_link') %></a>
                        </div>
                        <div class="goal-wrapper">
                            <div class="circle-progressbar-dark circle-progressbar" data-animate="false">
                                <div class="circle-bar" data-percent="98">
                                    <div class="circle-number"></div>
                                </div>
                            </div>
                            <h5 class="goal-title"><%= __('feature_page.goal_3_title') %></h5>
                            <span class="goal-info">
                                <%= __('feature_page.goal_3_desc') %>
                            </span>
                            <a href="/testimonial" class="goal-link"><%= __('feature_page.goal_3_link') %></a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
    <!-- footer part -->
<%- include('footer') %>
